{
  "name": "nailspa-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npx tsx dist/app.js",
    "dev": "npx tsx watch src/app.ts",
    "db:seed:generate": "set NODE_OPTIONS=--require ts-node/register &&sequelize-cli seed:generate --name",
    "db:seed:all": "set NODE_OPTIONS=--require ts-node/register && sequelize-cli db:seed:all",
    "db:drop": "set NODE_OPTIONS=--require ts-node/register && sequelize-cli db:drop",
    "db:create": "set NODE_OPTIONS=--require ts-node/register && sequelize-cli db:create",
    "db:migrate": "set NODE_OPTIONS=--require ts-node/register && sequelize-cli db:migrate",
    "db:relational:generate": "set NODE_OPTIONS=--require ts-node/register && sequelize-cli model:generate --",
    "db:migration:generate": "set NODE_OPTIONS=--require ts-node/register && sequelize-cli migration:generate --name",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npm run db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.1.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "@types/pg": "^8.15.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
